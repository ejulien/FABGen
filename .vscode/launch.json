{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "CPython: Tests --linux",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceRoot}/tests.py",
			"cwd": "${workspaceRoot}",
			"args": ["--linux", "--x64"],
			"env": null,
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		},
